{
  "name": "@compass-coach/cli",
  "version": "0.1.0",
  "description": "Closed-loop operations layer for Claude Code - Coach & Governor",
  "type": "module",
  "bin": {
    "compass": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'TODO: eslint'",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "commander": "^14.0.3"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "claude-code",
    "ai",
    "automation",
    "hooks",
    "coach",
    "governor"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joseph0926/compass"
  },
  "packageManager": "pnpm@10.28.2"
}
